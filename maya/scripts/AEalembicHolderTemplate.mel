global proc jsonImport()
{

    python("from alembicHolder.cmds import abcToApi");
    python("for i in abcToApi.getSelected(cls=True): i.importJson()");

}

global proc abcImport()
{

    python("from alembicHolder.cmds import abcToApi");
    python("for i in abcToApi.getSelected(cls=True): i.importAbc()");
}

global proc shadersImport()
{

    python("from alembicHolder.cmds import abcToApi");
